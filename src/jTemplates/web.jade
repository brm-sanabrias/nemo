
extends partials/base.jade

block title
	-var titulo = "Pantallas";
	title brm - Nemo

block content

	div.container.web


		div.row
			div.col.s12.m12.l12

				div.mask
					ul.content-pant
						- for (var x = 0; x < 10; x++)
							li.screen
								figure.z-depth-1
									span.mdi-navigation-check.hide
									img(src="images/sample-screen.jpg")

								p www.oli.com

			div.col.s6.m6.l6.push-l4
				
				button.waves-effect.waves-light.btn.green.mostrar-keyb.
					No es ninguna
					<i class="right mdi-hardware-keyboard"></i>

				button.valign.waves-effect.waves-light.btn.orange.darken-4.continuar.
					No tengo sitio web
					<i class="right mdi-navigation-close"></i>


	| {#include file='keyboard.html'#}



block Scripts
	script(src='js/nemo.web.js')